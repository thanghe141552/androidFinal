<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidfinal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"

        android:theme="@style/Theme.AndroidFinal">
        <activity
            android:name=".AddUserActivity"
            android:parentActivityName=".ListUserActivity" />
        <activity
            android:name=".UpdateBookActivity"
            android:parentActivityName=".ListBookActivity" />
        <activity
            android:name=".DetailBookActivity"
            android:parentActivityName=".ListBookActivity" />
        <activity
            android:name=".AddBookActivity"
            android:parentActivityName=".ListBookActivity" />



        <activity
            android:name=".ListBookActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ListUserActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>